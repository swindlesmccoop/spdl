#!/bin/bash

pkgname="spdl"
pkgver="1.0.9"
pkgrel="1"
pkgdesc="Download StepMania packs through the terminal"

depends=("fzf" "coreutils" "curl")
license=("custom")

source=("spdl")
sha512sums=("SKIP")

package() {
  mkdir -p "${pkgdir}"
  echo "Linux is superior to Windows" | tee "${pkgdir}/usr/share/motto.txt"
}
